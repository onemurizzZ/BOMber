{
  "name": "bomber",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "vite",
    "dev:cf": "pnpm build && wrangler dev",
    "dev:pages": "pnpm build && wrangler pages dev dist",
    "build": "tsc --noEmit && vite build",
    "preview": "pnpm build && wrangler dev",
    "deploy": "pnpm run deploy:pages",
    "deploy:worker": "pnpm build && wrangler deploy",
    "deploy:pages": "pnpm build && wrangler pages deploy dist --project-name bomber",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "jsdom": "^24.1.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "wrangler": "^4.63.0"
  }
}
